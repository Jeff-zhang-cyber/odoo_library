<?xml version="1.0"?>
<odoo>
	<record id="view_form_product_template_extend" model="ir.ui.view">
		<field name="name">Sparts:secret?</field>
		<field name="model">product.template</field>
		<field name="inherit_id" ref="product.product_template_form_view" />
		<field name="arch" type="xml">
			<field name="sale_ok" position="after">
				<field name="isprivate"/>
			</field>
		</field>
	</record>
    <record id="view_form_product_tree_extend" model="ir.ui.view">
        <field name="name">Sparts:Sn and Type</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_only_form_view" />
        <field name="arch" type="xml">
            <field name="barcode" position="attributes">
                <attribute name="invisible">True</attribute>
            </field>
            <field name="barcode" position="after">
                <field name="serialNo" />
                <field name="partstype"/>
            </field>
        </field>
    </record>
</odoo>
