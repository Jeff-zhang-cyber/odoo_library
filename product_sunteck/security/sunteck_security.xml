<?xml version="1.0" ?>
<odoo>
    <record id="module_sunteck_category" model="ir.module.category">
        <field name="name">Sunteck</field>
    </record>
	
	<!-- Sunteck User Group -->
    <record id="sunteck_group_user" model="res.groups">
        <field name="name">User</field>
        <field name="category_id" ref="module_sunteck_category" />
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]" />
    </record>
    <!-- Sunteck Manager Group -->
    <record id="sunteck_group_manager" model="res.groups">
        <field name="name">Manager</field>
        <field name="category_id" ref="module_sunteck_category" />
        <field name="implied_ids" eval="[(4, ref('sunteck_group_user'))]" />
        <field name="users" eval="[
                    (4, ref('base.user_root')),
                    (4, ref('base.user_admin'))
                ]" />
    </record>
</odoo>